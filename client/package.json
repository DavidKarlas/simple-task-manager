{
  "name": "simple-task-manager",
  "version": "1.3.0-dev",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --watch",
    "dev-ja": "ng serve --watch --configuration=ja",
    "dev-de": "ng serve --watch --configuration=de",
    "dev-zh": "ng serve --watch --configuration=zh-CN",
    "dev-fr": "ng serve --watch --configuration=fr",
    "dev-local": "ng serve --watch -c local",
    "build": "ng build --localize",
    "build-prod": "ng build --prod --localize",
    "build-test": "ng build -c test --prod --localize",
    "test-firefox": "ng test --browsers=FirefoxHeadless --code-coverage",
    "test": "ng test --source-map --watch=true --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0",
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/forms": "^11.1.0",
    "@angular/localize": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@angular/platform-browser-dynamic": "^11.1.0",
    "@angular/router": "^11.1.0",
    "@turf/hex-grid": "^6.3.0",
    "@turf/square-grid": "^6.3.0",
    "@turf/triangle-grid": "^6.3.0",
    "@types/ol": "^6.4.2",
    "ol": "^6.5.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.1.0",
    "@angular/compiler-cli": "^11.1.0",
    "@types/jasmine": "^3.6.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.0",
    "typescript": "~4.0.2"
  }
}
