{
  "name": "simple-task-manager",
  "version": "1.4.2",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --watch",
    "dev-ja": "ng serve --watch --configuration=ja",
    "dev-de": "ng serve --watch --configuration=de",
    "dev-zh": "ng serve --watch --configuration=zh-CN",
    "dev-fr": "ng serve --watch --configuration=fr",
    "dev-es": "ng serve --watch --configuration=es",
    "dev-local": "ng serve --watch -c local",
    "build": "ng build -c production --localize",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test-chrome": "ng test --source-map --watch=true --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "lint": "ng lint --fix",
    "i18n": "ng extract-i18n --output-path src/locale",
    "update": "npx npm-check-updates -u && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.2",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/forms": "^13.2.6",
    "@angular/localize": "^13.2.6",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.2.6",
    "@angular/router": "^13.2.6",
    "@turf/hex-grid": "^6.5.0",
    "@turf/square-grid": "^6.5.0",
    "@turf/triangle-grid": "^6.5.0",
    "ng-mocks": "^13.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "ol": "^6.13.0",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.6",
    "@angular/cli": "^13.2.6",
    "@angular/compiler-cli": "^13.2.3",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.0.1",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
